{
  "extends": "@farcaster/tsconfig/browser.json",
  "include": ["src"],
  "exclude": ["esm", "dist", "build", "node_modules"],
  "compilerOptions": {
    "outDir": "esm",
    "module": "esnext",
    "moduleResolution": "node",
    "declaration": true,
    "emitDeclarationOnly": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "esModuleInterop": true
  }
}
