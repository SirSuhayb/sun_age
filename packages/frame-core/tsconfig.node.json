{
  "extends": "@farcaster/tsconfig/node.json",
  "include": ["src"],
  "exclude": ["esm", "dist", "build", "node_modules"],
  "compilerOptions": {
    "outDir": "dist",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "declaration": true,
    "emitDeclarationOnly": false
  }
}

